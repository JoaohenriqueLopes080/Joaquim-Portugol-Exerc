algoritmo tabuada5

    inicio
        inteiro numero, i, resultado;

        i = 0;
        numero = 5;

        escreva("Tabuada do ", numero, ":\n");
        
        para i de 1 ate 10 faca {
            resultado = numero * i;
            escreva(numero, " x ", i, " = ", resultado, "\n");
        fim_para }
        
    Fim