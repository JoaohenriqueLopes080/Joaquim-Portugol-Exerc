algoritmo dados
    inicio
    
        inteiro : nome, endereço, telefone;

        escreva("Digite seu nome: ");
            leia(nome);

        escreva("Digite seu endereço: ");
            leia(endereço);

        escreva("Digite seu telefone: ");
            leia(telefone);

        escreva("Nome: ", nome, "\n");
        escreva("Endereço: ", endereço, "\n");
        escreva("Telefone: ", telefone, "\n");
    fim
algoritmo

