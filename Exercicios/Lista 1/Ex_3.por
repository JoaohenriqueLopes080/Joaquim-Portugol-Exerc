algorito produto
  inicio
  
    inteiro numero1, numero2, produto;

    escreva("Digite o primeiro numero: ");
    leia(numero1);

    escreva("Digite o segundo numero: ");
    leia(numero2);

    produto = numero1 * numero2;

    escreva("O produto de ", numero1, " e ", numero2, " eh: ", produto);
    
    fim
algoritmo